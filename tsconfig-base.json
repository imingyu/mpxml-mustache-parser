{
    "$schema": "http://json.schemastore.org/tsconfig",
    "$title": "基础配置",
    "include": ["./*.js", "./src/**/*.ts", "./test/**/*.ts"],
    "compilerOptions": {
        "allowJs": false,
        "strictNullChecks": true,
        "moduleResolution": "Node",
        "downlevelIteration": true,
        "module": "ESNext",
        "target": "es2022",
        "types": [],
        "typeRoots": []
    }
}
